//*******************************************create common request for browser support*****************************//
function createRequest()
{
    var ro;
    var browser = navigator.appName;
    if(browser == "Microsoft Internet Explorer")
    {
        ro = new ActiveXObject("Microsoft.XMLHTTP");
    }
    else
    {
        ro = new XMLHttpRequest();
    }
    return ro;
}
var httpreqTextc = createRequest();
var param = "";
//**********************************common ajax function**********************************************************//
function callCommonAjaxc(callbackfunction, url, Qstring)
{
  var contentType = "application/x-www-form-urlencoded;charset=UTF-8";
  httpreqTextc.open('POST',url+Qstring,true);
  httpreqTextc.setRequestHeader("Content-Type", contentType);
  httpreqTextc.onreadystatechange = callbackfunction;
  httpreqTextc.send(Qstring);
}
//***********analytics code********************************//
function analyticsRoadTrip(slinkName,slinkType){
	customLinkClickAnalytics(slinkName, slinkType);
}
function getRoadTripHomeDetails(){
	param = "10";
	var Qstring="?methodId="+param;
	callCommonAjaxc(setRoadTripHomeDetails,'/tourism/roadTrip', Qstring);
}
